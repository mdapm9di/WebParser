{
  "name": "webparser",
  "version": "1.0.0",
  "description": "Универсальный веб-парсер",
  "main": "frontend/main.js",
  "scripts": {
    "build-css": "sass frontend/src/styles/scss:frontend/src/styles/css --watch --no-source-map",
    "build-css-once": "sass frontend/src/styles/scss:frontend/src/styles/css --no-source-map",
    "start": "concurrently \"npm run build-css\" \"electron .\"",
    "build": "npm run build-css-once && electron-builder",
    "dev": "concurrently \"npm run build-css\" \"python backend/app.py\" \"wait-on http://localhost:5000 && electron .\"",
    "install-deps": "pip install -r requirements.txt",
    "install-all": "npm install && npm run install-deps",
    "dev-fix": "concurrently \"npm run build-css\" \"python backend/app.py\" \"wait-on http://localhost:5000/ && electron .\"",
    "dev-with-flask": "concurrently \"npm run build-css\" \"electron .\""
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "9.2.1",
    "electron": "38.1.0",
    "electron-builder": "26.0.20",
    "postcss": "^8.5.6",
    "sass": "^1.77.2",
    "wait-on": "8.0.4"
  },
  "dependencies": {
    "all": "^0.0.0",
    "axios": "1.12.0"
  }
}